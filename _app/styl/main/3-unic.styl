#unics
	padding 100px 0
.unics-list
	margin-bottom: 160px
	width 50%
	li
		list-style: none
		margin-bottom: 35px
		position relative
		padding-left: 28px
		&:before
			content: ''
			display block
			position absolute
			border-radius: 50%
			background-color: #E4845A
			width 50px
			height: 50px
			top -11px
			left 0
			z-index -1
.unics-img
	position absolute
	top 66px
	right 0
	z-index -1
	width 50%